@codable()
@tokenize()
enum Token {

  @token(pattern: "FROM")
  case from()

  @token(pattern: "SELECT")
  case select()

  @token(pattern: "WHERE")
  case where()

  @token(pattern: "([a-zA-Z]\w*)")
  case identifier(value: String)

  @token(pattern: "(0|[1-9]\d*)")
  case integer(value: String)
  
  @token(pattern: "\s+")
  case whitespace()
}